<div class="container">
  <div class="row">
    <div class="col-xs-offset-1 col-xs-10 col-sm-offset-3 col-sm-6 col-lg-offset-4 col-lg-4">
      <%= bootstrap_form_for([@flat, @booking], class: 'form') do |f| %>
        <div class="field">
          <%= f.date_field :start_date %>
        </div>
         <div class="field">
          <%= f.date_field :end_date %>
        </div>
        <div class="actions">
          <%= f.submit  %>
        </div>
      <% end %>
    </div>
  </div>
</div>


